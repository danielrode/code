import sys
from dateutil.parser import parse as dparse
from sys import exit


def print2(*args, **kwargs):
    print(*args, **kwargs, file=sys.stderr)


args = sys.argv[1:]
try:
    date1_str = args[0]
    date2_str = args[1]
except IndexError:
    print2(HELP_TEXT)
    exit(1)

time_delta = dparse(date2_str)-dparse(date1_str)
print(str(time_delta))
